{% form_theme form 'bootstrap_4_layout.html.twig' %}



{{ form_start(form, {'attr': {class: 'js-new-user-form', novalidate: 'novalidate'}}) }}
{{ form_row(form.firstName, {'attr': {autofocus: 'autofocus'}}) }}
{{ form_row(form.lastName) }}
{{ form_row(form.email) }}
{{ form_row(form.password) }}
{{ form_row(form.passwordRepeat) }}
{{ form_row(form.customRoles) }}
{{ form_row(form.isAdminUser) }}
{{ form_row(form.isActive) }}

<br>
{{ form_rest(form) }}
{{ form_end(form) }}
