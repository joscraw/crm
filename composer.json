{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "auth0/auth0-php": "^7.1",
        "blackfire/php-sdk": "^1.22",
        "box/spout": "^3.1",
        "doctrine/doctrine-bundle": "^2.0",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "friendsofsymfony/jsrouting-bundle": "^2.2",
        "google/apiclient": "^2.0",
        "lexik/form-filter-bundle": "^5.0",
        "lexik/jwt-authentication-bundle": "^2.6",
        "lightsaml/lightsaml": "^1.4",
        "nelmio/api-doc-bundle": "^3.6",
        "nscs/nscs-bundle": "*@dev",
        "oneup/flysystem-bundle": "^3.4",
        "php-mime-mail-parser/php-mime-mail-parser": "^5.0",
        "phpoffice/phpspreadsheet": "^1.10",
        "rollerworks/password-strength-bundle": "^2.0",
        "scienta/doctrine-json-functions": "^4.0",
        "sensio/framework-extra-bundle": "^5.2",
        "stof/doctrine-extensions-bundle": "^1.4",
        "symfony/amqp-pack": "^1.0",
        "symfony/asset": "4.4.*",
        "symfony/cache": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/messenger": "4.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "4.4.*",
        "symfony/requirements-checker": "^1.1",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "4.4.*",
        "white-october/pagerfanta-bundle": "^1.3"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "symfony/browser-kit": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "4.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "NSCSBundle\\": "lib/NSCSBundle/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "requirements-checker": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    },
    "repositories": [
      {
        "type": "path",
        "url": "NSCSBundle"
      }
    ]
}
